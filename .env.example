#Server Configuration
PORT=3000
NODE_ENV=development

# Camera Configuration (gphoto2 based)
# Note: USE_DSLR, USE_WEBCAM, DSLR_MODEL, DSLR_PREVIEW_MODE, WEBCAM_PREVIEW, PREFERRED_DEVICES, FIXED_CAMERA_PATH are no longer used.
VERBOSE_LOGGING=false
DEBUG_MODE=false  # Set to true to enable debug overlay and enhanced logging
LOGO_SIZE_PERCENT=11.0  # Size of the watermark logo as percentage of image width

# SMS Service Configuration
USE_MOCK_SMS=true

# AWS Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=

# AWS S3 Configuration
S3_BUCKET_NAME=

# AWS SNS Configuration
SNS_TOPIC_ARN=your_sns_topic_arn
SNS_SENDER_ID=

# Telnyx Configuration
TELNYX_API_KEY=
TELNYX_PHONE_NUMBER=
TELNYX_MESSAGING_PROFILE_ID=

# Email Configuration
EMAIL_FROM=photobooth@example.com

# Server Configuration
BASE_URL=http://localhost
